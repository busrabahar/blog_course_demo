 <h1 id= "page_title"> Yazdigim Seyler </h1>
 <div id="posts_wrapper" class="skinny_wrapper">
        <% @posts.each do |post| %>
        <div class="post">
            <p class="date"><%= post.created_at.strftime('%A, %B %d') %> </p>
            <h2><a href="#"><%= link_to post.title, post %></a></h2>
            <hr>
        </div>
        <% end %>
        </div>
       
        <div class="pagination">
            <span class="previus_page disabled">Previous</span>
            <em class="current">1</em>
            <a rel="next" href="#">Next</a>
        </div>

    